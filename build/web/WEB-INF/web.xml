<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoneModel</servlet-name>
        <servlet-class>servlets.ZoneModel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Buscador</servlet-name>
        <servlet-class>servlets.Buscador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modificador</servlet-name>
        <servlet-class>servlets.Modificador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Guardar</servlet-name>
        <servlet-class>servlets.Guardar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Eliminar</servlet-name>
        <servlet-class>servlets.Eliminar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RumConfiguration</servlet-name>
        <servlet-class>servlets.RumConfiguration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BalanceElement</servlet-name>
        <servlet-class>servlets.BalanceElement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rollover</servlet-name>
        <servlet-class>servlets.Rollover</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeModel</servlet-name>
        <servlet-class>servlets.TimeModel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoneModel</servlet-name>
        <url-pattern>/zoneModels</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Buscador</servlet-name>
        <url-pattern>/buscar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modificador</servlet-name>
        <url-pattern>/modificar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Guardar</servlet-name>
        <url-pattern>/guardar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Eliminar</servlet-name>
        <url-pattern>/eliminar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RumConfiguration</servlet-name>
        <url-pattern>/rumConfig</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BalanceElement</servlet-name>
        <url-pattern>/balanceElement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rollover</servlet-name>
        <url-pattern>/rollover</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeModel</servlet-name>
        <url-pattern>/timeModel</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Login</welcome-file>
    </welcome-file-list>
</web-app>
